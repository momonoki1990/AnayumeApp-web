<div class="container show-follow">
  <div class="row">
    <div class="col-md-6">
      <div class="sticky">
        <% if logged_in? %>
        <%= render "static_pages/after_logged_in_home" %>
        <% else %>
        <%= render "static_pages/before_logged_in_home" %>
        <% end %>
      </div>
    </div>
    <div class="col-md-6 mt">
      <h3>ユーザー一覧</h3>
      <div class="show-follows jscroll">
        <%= render @users %>
        <%= paginate @users %>
      </div>
    </div>
  </div>
</div>