<div class="container home">
  <div class="row">
    <div class="col-md-6">
      <div class="sticky">
        <% if logged_in? %>
        <%= render "static_pages/after_logged_in_home" %>
        <% else %>
        <%= render "static_pages/before_logged_in_home" %>
        <% end %>
      </div>
    </div>
    <div class="col-md-6 mt">
      <div class="d-flex justify-content-end mb-2">
        <%= render "shared/search_form" %>
      </div>
      <%= render "shared/feed" %>
    </div>
  </div>
</div>